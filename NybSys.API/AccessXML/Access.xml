<?xml version="1.0"?>
<AccessRights xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <RoleName>Admin</RoleName>
  <RightLists>
    <AccessControl>
      <ControllerName>LogController</ControllerName>
      <ActionName>
        <string>GetAuditLogByFilter</string>
      </ActionName>
    </AccessControl>
    <AccessControl>
      <ControllerName>SecurityController</ControllerName>
      <ActionName>
        <string>CreateUser</string>
        <string>UpdateUser</string>
        <string>GetUser</string>
        <string>ChangePassword</string>
        <string>ResetPassword</string>
        <string>Login</string>
        <string>Logout</string>
        <string>GetSessionLogByFilter</string>
        <string>GetUsersByFilter</string>
        <string>CancelSession</string>
      </ActionName>
    </AccessControl>
    <AccessControl>
      <ControllerName>ValuesController</ControllerName>
      <ActionName>
        <string>Get</string>
        <string>Get</string>
        <string>Post</string>
        <string>Put</string>
        <string>Delete</string>
        <string>GetActionList</string>
      </ActionName>
    </AccessControl>
  </RightLists>
</AccessRights>